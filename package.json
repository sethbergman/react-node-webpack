{
  "name": "react-node-webpack",
  "description": "Example app built with React, Node.js and Webpack",
  "author": "Seth Bergman <hello@sethbergman.com>",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "webpack -d --progress --colors --watch",
    "dist": "webpack -p",
    "postinstall": "npm run dist",
    "test": "jest",
    "dev": "nodemon src/server.js",
    "start": "node src/server.js"
  },
  "dependencies": {
    "classnames": "^2.1.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^7.8.2",
    "express": "^4.12.4",
    "express-handlebars": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsx-loader": "^0.13.2",
    "less": "^2.4.0",
    "less-loader": "^2.0.0",
    "normalize.css": "^5.0.0",
    "object-assign": "^4.1.1",
    "qs": "^6.3.0",
    "react": "^15.4.2",
    "react-router": "^3.0.2",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.10.5"
  },
  "devDependencies": {
    "jest-cli": "^22.1.0",
    "nodemon": "^1.3.7",
    "react-tools": "^0.13.3"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/support/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
